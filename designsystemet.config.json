{
  "$schema": "node_modules/@digdir/designsystemet/dist/config.schema.json",
  "outDir": "./design-tokens",
  "themes": {
    "theme": {
      "colors": {
        "main": {
          "accent": "#0000ffab"
        },
        "support": {
          "brand1": "#0d0f7a",
          "brand2": "#9ca03f"
        },
        "neutral": "#b704b7"
      },
      "overrides": {
        "severity": {
          "success": "#ffb608",
          "warning": "#6d4402"
        },
        "colors": {
          "accent": {
            "text-subtle": {
              "light": "#ff0000ab",
              "dark": "#ff0000ac"
            },
            "base-hover": {
              "light": "#ff0000ad",
              "dark": "#ff0000ae"
            }
          },
          "info": {
            "border-subtle": {
              "light": "#00ff00",
              "dark": "#00ff88"
            }
          }
        }
      },
      "borderRadius": 4
    }
  }
}
